{
  "mcpServers": {
    "matlab": {
      "type": "stdio",
      "command": "/Users/michellehirsch/Coding/mcpserver/matlab-mcp-core-server",
      "args": [
        "--matlab-root",
        "/Applications/MATLAB_R2026a.app",
        "--matlab-display-mode",
        "nodesktop"
      ],
      "env": {}
    }
  },
  "github": {
    "type": "stdio",
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-github"],
    "env": {
      "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
    }
  },
  "filesystem": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-filesystem",
      "${PROJECT_ROOT:-.}"
    ],
    "env": {}
  }
}
